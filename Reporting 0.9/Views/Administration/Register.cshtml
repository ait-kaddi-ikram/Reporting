@model Reporting.ViewModels.AccountRegisterViewModel;

@{
    ViewData["Title"] = "Ajouter Un Utlisateur";
    var md = ViewBag.model;
}
<div class="container">
<h1>Ajouter Un Utlisateur</h1>

<div class="row">
    <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Prenom"></label>
                <input asp-for="Prenom" class="form-control" style="width:500px" />
                <span asp-validation-for="Prenom" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nom"></label>
                <input asp-for="Nom" class="form-control" style="width:500px"/>
                <span asp-validation-for="Nom" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Site"></label>
                <input asp-for="Site" class="form-control" style="width:500px" />
                <span asp-validation-for="Site" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Direction"></label>
                <input asp-for="Direction" class="form-control" style="width:500px"/>
                <span asp-validation-for="Direction" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control" style="width:500px" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Password"></label>
                <input asp-for="Password" class="form-control" style="width:500px" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="ConfirmPassword"></label>
                <input asp-for="ConfirmPassword" class="form-control" style="width:500px" />
                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
            </div>




            <button type="submit" class="btn-primary"  style="width:100px;" >Eenrigestrer</button>
         
        </form>
        

    </div>
</div>
    </div>
